{
  "type": "object",
  "required": [
    "firstName",
    "lastName",
    "email",
    "password"
  ],
  "properties": {
    "id": {
      "type": "string"
    },
    "firstName": {
      "type": "string",
      "example": "John"
    },
    "lastName": {
      "type": "string",
      "example": "Doe"
    },
    "email": {
      "type": "string",
      "example": "john@doe.com"
    },
    "password": {
      "type": "string",
      "example": "123"
    },
    "favourites": {
      "type": "array",
      "items": {
        "type": "number"
      }
    }
  }
}